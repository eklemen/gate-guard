    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-typescript": "^0.14.0",
    "express": "^4.17.1",
    "jest": "^26.0.1",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "tslib": "^2.0.3",
    "typescript": "^3.9.3",
